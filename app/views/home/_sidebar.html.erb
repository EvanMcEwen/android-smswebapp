<div class="container-fluid">
  <div class="row-fluid">
        <div class="span3">
        <h2>Conversations</h2>
        <% if !@conversations.blank? %>
            <ul class="nav nav-tabs nav-stacked nav-conversation">
          <% for item in @conversations %>
            <li id="<%= item %>_nav"><a href="javascript:switchConversation(<%= item %>)"><%= item %></a></li>
          <% end %>
            </ul>
        <% else %>
        <% end %>
        </div>